<script>
  function showparticipants(){
    var element=document.getElementById("show-part");
    var element_small=document.getElementById("me");
    var my_tab=document.getElementById("t");
    if (element.style.display === "none" && element_small.style.display==="none") {
        element.style.display = "block";
        element.style.display = "flex";
        element_small.style.display = "block";
        element_small.style.display = "flex";
        my_tab.style.width="94.5%";
      } else {
        element.style.display = "none";
        element_small.style.display = "none";
        my_tab.style.width="91.5%";
      }

  }

  function outit(){

    var element=document.getElementById("vid_desc");
    element.style.display="none";
  }

  function onthat()
  {
    var element=document.getElementById("vid_desc");
    element.style.display="block";
  }

  function clickedme()
  {
      var section = document.getElementById("vid_desc");
      if (section.style.display === "none") {
        section.style.display = "block";
      } else {
        section.style.display = "none";
      }
  }

</script>
